<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangz.springbootdemosql.mapper.AgencyInfoMapper">

    <sql id="tableName"> C_OC_B_AGENCY_INFO</sql>
    <select id="querybyAgencyName" resultType="entity.AgencyInfo">
        select * from
        <include refid="tableName"/>
        <where>
            AGENCY_NAME = #{agencyName,jdbcType=VARCHAR}
        </where>
    </select>
</mapper>
